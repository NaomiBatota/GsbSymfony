{% extends "PgGsbFraisBundle::accueil2.html.twig" %}
{% block menu %}



{{parent()}}
<div id="contenu">
      
  <table classe="listeLegere">
         <tr>
           <th> Visiteurs</th>
           <th> Numéro Tablette</th>
           <th> Type de Tablette</th>
           <th> Mémoire Interne</th>
           <th> Mémoire Externe</th>
           <th> Date d'Affectation</th>
         </tr>
         {%for uneListe in lesTablettes%} 
         <tr>
           <td>{{uneListe.refVisiteur}}</td>
           <td>{{uneListe.idtablette}}</td>
           <td>{{uneListe.type}}</td>
           <td>{{uneListe.memoire}} </td>
           <td>{{uneListe.memoireExt}}</td>
           <td>{{uneListe.dateAffectation}}</td>
         </br>
     </tr>
     {% endfor %}
  </table>

</div> 

{% endblock %}